{
  "name": "start-standard",
  "version": "1.0.0",
  "description": "Standard Style checker for Start",
  "keywords": [
    "start",
    "start-task",
    "standard",
    "semistandard",
    "happiness",
    "lint"
  ],
  "main": "dist/index.js",
  "files": [
    "dist/"
  ],
  "homepage": "https://github.com/effervescentia/start-standard",
  "repository": "effervescentia/start-standard",
  "author": "Ben Teichman <ben.l.teichman@gmail.com> (https://github.com/effervescentia)",
  "scripts": {
    "start": "start-runner -p re-start/presets/es6",
    "prepush": "yarn start prepush",
    "prepublish": "yarn start build"
  },
  "release": {
    "verifyConditions": "condition-circle"
  },
  "engines": {
    "node": ">=4"
  },
  "license": "MIT",
  "dependencies": {
    "chalk": "^1.1.3",
    "happiness": "^7.1.2",
    "semistandard": "^9.2.1",
    "standard": "^8.6.0",
    "text-table": "^0.2.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^3.18.0",
    "eslint-config-tough": "^0.4.0",
    "eslint-plugin-promise": "^3.5.0",
    "start-simple-cli": "^4.1.1",
    "tkstr-devtools": "^1.2.2"
  },
  "peerDependencies": {
    "happiness": "^7.1.2",
    "semistandard": "^10.0.0",
    "standard": "^9.0.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
