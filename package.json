{
  "name": "start-standard",
  "version": "1.0.0",
  "description": "Standard Style checker for Start",
  "keywords": [
    "start",
    "start-task",
    "standard",
    "semistandard",
    "happiness",
    "lint"
  ],
  "main": "build/index.js",
  "homepage": "https://github.com/effervescentia/start-standard",
  "repository": "effervescentia/start-standard",
  "author": "Ben Teichman <ben.l.teichman@gmail.com> (https://github.com/effervescentia)",
  "files": [
    "build/"
  ],
  "scripts": {
    "start": "start-runner -p start-standard-preset",
    "prepush": "yarn start prepush",
    "prepublish": "yarn start build",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "release": {
    "verifyConditions": "condition-circle"
  },
  "engines": {
    "node": ">=4"
  },
  "license": "MIT",
  "dependencies": {
    "chalk": "^1.1.3",
    "text-table": "^0.2.0"
  },
  "devDependencies": {
    "babel-preset-start": "^0.1.0",
    "condition-circle": "^1.5.0",
    "cz-conventional-changelog": "^1.2.0",
    "husky": "^0.13.0",
    "semantic-release": "^6.3.2",
    "start-babel-cli": "^4.0.1",
    "start-standard-preset": "^1.1.0",
    "tape": "^4.6.3"
  },
  "peerDependencies": {
    "happiness": "^7.1.2",
    "semistandard": "^9.2.1",
    "standard": "^8.6.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
